{{>navbar}}
<div class="main-page artist-search">
    {{#each Album}}
        <a href="/albums/tracks/{{id}}" class="artist-link">
            <div class="artist">
                <img src="{{images.[0].url}}" alt="{{name}}">
                <div class="name-section">
                    <h3 class="artist-name">{{name}}</h3>
                    <button>View Tracks</button>
                </div>
            </div>
        </a>
    {{/each}}
</div>